<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="LocalDebug"
	SccProjectName=""
	SccLocalPath="">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Release|Win32"
			OutputDirectory=".\..\..\Release"
			IntermediateDirectory=".\WinRel"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1"
			WholeProgramOptimization="TRUE">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="1"
				GlobalOptimizations="FALSE"
				InlineFunctionExpansion="1"
				ImproveFloatingPointConsistency="FALSE"
				FavorSizeOrSpeed="0"
				OmitFramePointers="TRUE"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="..\i386CPUHelp,..\..\Include,..\Include,..\LocalAssist"
				PreprocessorDefinitions="NDEBUG;WIN32;_WINDOWS;_USRDLL;BUILDING_DEBUGINTERFACE_DLL"
				StringPooling="TRUE"
				MinimalRebuild="FALSE"
				BasicRuntimeChecks="0"
				SmallerTypeCheck="FALSE"
				RuntimeLibrary="2"
				StructMemberAlignment="0"
				BufferSecurityCheck="TRUE"
				EnableFunctionLevelLinking="TRUE"
				UsePrecompiledHeader="3"
				PrecompiledHeaderThrough="PCH.h"
				PrecompiledHeaderFile=".\WinRel/LocalDebug.pch"
				AssemblerListingLocation=".\WinRel/"
				ObjectFile=".\WinRel/"
				ProgramDataBaseFileName="$(IntDir)"
				WarningLevel="4"
				WarnAsError="TRUE"
				SuppressStartupBanner="TRUE"
				Detect64BitPortabilityProblems="TRUE"
				DebugInformationFormat="3"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="USER32.LIB PSAPI.LIB DBGHELP.lib $(NOINHERIT)"
				OutputFile=".\..\..\Release/LocalDebug.dll"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="$(OutDir)"
				ModuleDefinitionFile=".\$(ProjectName).DEF"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile="$(OutDir)/$(ProjectName).pdb"
				StripPrivateSymbols="$(OutDir)/$(ProjectName)_STRIPPED.pdb"
				GenerateMapFile="TRUE"
				MapFileName="$(OutDir)/$(ProjectName).map"
				MapExports="TRUE"
				MapLines="TRUE"
				OptimizeReferences="2"
				EnableCOMDATFolding="2"
				OptimizeForWindows98="1"
				SetChecksum="TRUE"
				BaseAddress="@..\DLLBaseAddresses.txt,LocalDebug"
				ImportLibrary="$(OutDir)/$(ProjectName).lib"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\..\..\Release/LocalDebug.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory=".\..\..\Output"
			IntermediateDirectory=".\WinDebug"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				AdditionalIncludeDirectories="..\i386CPUHelp,..\..\Include,..\Include,..\LocalAssist"
				PreprocessorDefinitions="_DEBUG;WIN32;_WINDOWS;_USRDLL;BUILDING_DEBUGINTERFACE_DLL"
				BasicRuntimeChecks="3"
				SmallerTypeCheck="TRUE"
				RuntimeLibrary="3"
				StructMemberAlignment="0"
				BufferSecurityCheck="TRUE"
				UsePrecompiledHeader="3"
				PrecompiledHeaderThrough="PCH.h"
				PrecompiledHeaderFile=".\WinDebug/LocalDebug.pch"
				AssemblerListingLocation=".\WinDebug/"
				ObjectFile=".\WinDebug/"
				ProgramDataBaseFileName=".\WinDebug/"
				WarningLevel="4"
				WarnAsError="TRUE"
				SuppressStartupBanner="TRUE"
				Detect64BitPortabilityProblems="TRUE"
				DebugInformationFormat="3"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="USER32.LIB PSAPI.LIB DBGHELP.lib $(NOINHERIT)"
				OutputFile=".\..\..\Output/LocalDebug.dll"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="$(OutDir)"
				ModuleDefinitionFile=".\$(ProjectName).DEF"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile="$(OutDir)/$(ProjectName).pdb"
				OptimizeForWindows98="1"
				BaseAddress="@..\DLLBaseAddresses.txt,LocalDebug"
				ImportLibrary="$(OutDir)/$(ProjectName).lib"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="_DEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\..\..\Output/LocalDebug.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;r;odl;idl;hpj;bat">
			<File
				RelativePath=".\CreateDebugSyncEvents.cpp">
			</File>
			<File
				RelativePath=".\DebugThread.cpp">
			</File>
			<File
				RelativePath=".\DebuggeeInfo.cpp">
			</File>
			<File
				RelativePath=".\DllMain.cpp">
			</File>
			<File
				RelativePath=".\LocalDebug.def">
			</File>
			<File
				RelativePath=".\PCH.cpp">
				<FileConfiguration
					Name="Release|Win32">
					<Tool
						Name="VCCLCompilerTool"
						UsePrecompiledHeader="1"/>
				</FileConfiguration>
				<FileConfiguration
					Name="Debug|Win32">
					<Tool
						Name="VCCLCompilerTool"
						UsePrecompiledHeader="1"/>
				</FileConfiguration>
			</File>
			<File
				RelativePath=".\ProcessDebugEvents.cpp">
			</File>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h;hpp;hxx;hm;inl">
			<File
				RelativePath=".\CreateDebugSynchEvents.h">
			</File>
			<File
				RelativePath="..\Include\DebugInterface.h">
			</File>
			<File
				RelativePath=".\DebuggeeInfo.h">
			</File>
			<File
				RelativePath=".\Dll.h">
			</File>
			<File
				RelativePath=".\PCH.h">
			</File>
			<File
				RelativePath=".\ProcessDebugEvents.h">
			</File>
			<File
				RelativePath=".\resource.h">
			</File>
		</Filter>
		<Filter
			Name="Resource Files"
			Filter="ico;cur;bmp;dlg;rc2;rct;bin;rgs;gif;jpg;jpeg;jpe">
			<File
				RelativePath=".\LocalDebug.rc">
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
